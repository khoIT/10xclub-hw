# digital_root
def sum_digit(num):
  if num < 10:
    return num
  return num % 10 + sum_digit(num/10)
def digital_root(num):
  while num > 9:
    num = sum_digit(num)
  return num

# fibs
def fibs(num):
  return [] if num == 0
  return [1] if num == 1
  return [1, 1] if num == 2
  arr = [1, 1]
  for i in xrange(3, num):
    arr[i] = arr[i-1] + arr[i-2]
  return arr
